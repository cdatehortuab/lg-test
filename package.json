{
  "name": "lg-test",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "basic-package": "ares-package build -o bundle -e asset-manifest.json -e precache-manifest.*.js -e service-worker.js",
    "dev-bundle": "yarn build && yarn basic-package --no-minify",
    "bundle": "yarn build && yarn basic-package -e *.map",
    "dev-install": "ares-install bundle/me.cdatehortuab.lg-app_0.0.1_all.ipk",
    "enable-chrome-debug": "ares-novacom -f -p 9998:9222",
    "launch": "ares-launch me.cdatehortuab.lg-app",
    "bundle-refresh": "yarn dev-bundle && yarn dev-install && yarn launch",
    "bundle-start": "yarn bundle-refresh && yarn enable-chrome-debug"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
